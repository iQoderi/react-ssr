{
  "name": "react-ssr",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "copyfile": "copyfiles -f ./client/index.html ./dist/views",
    "clean": "rimraf ./dist",
    "dev": "cross-env NODE_ENV=dev node ./server.js",
    "server": "cross-env NODE_ENV=production nodemon ./server.js",
    "client:dev": "cross-env NODE_ENV=dev node ./server.js",
    "server:dev": "cross-env NODE_ENV=dev  nodemon ./server/server.dev.js",
    "server:prod": "cross-env NODE_ENV=production  webpack --config ./build/webpack.config.prod.server.js  --progress",
    "client:prod": "npm run copyfile && cross-env NODE_ENV=production webpack --config ./build/webpack.config.prod.client.js  --progress",
    "build": "npm run clean && npm run client:prod && npm run server:prod",
    "test": "mocha"
  },
  "dependencies": {
    "co-body": "^4.0.0",
    "co-views": "^2.1.0",
    "ejs": "^2.5.6",
    "es6-promise": "^4.0.5",
    "fastclick": "^1.0.6",
    "fs": "0.0.1-security",
    "isomorphic-fetch": "^2.2.1",
    "koa": "^2.0.1",
    "koa-bodyparser": "^4.1.0",
    "koa-compress": "^1.0.9",
    "koa-convert": "^1.2.0",
    "koa-favicon": "^2.0.0",
    "koa-json": "^2.0.2",
    "koa-logger": "^1.3.1",
    "koa-route": "^2.4.2",
    "koa-static": "^2.0.0",
    "koa-views": "^6.0.0",
    "q": "^1.4.1",
    "react": "^15.4.2",
    "react-addons-pure-render-mixin": "^15.4.2",
    "react-addons-shallow-compare": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router": "2.8.1",
    "redux": "^3.6.0",
    "redux-saga": "^0.14.3",
    "swig": "^1.3.2"
  },
  "devDependencies": {
    "asset-require-hook": "^1.0.2",
    "autoprefixer": "^6.7.6",
    "babel-jest": "18.0.0",
    "babel-loader": "^6.3.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-native": "1.9.1",
    "babel-preset-stage-0": "^6.5.0",
    "better-npm-run": "0.0.14",
    "copyfiles": "^1.2.0",
    "cross-env": "^3.2.3",
    "css-loader": "^0.26.1",
    "css-modules-require-hook": "^4.0.5",
    "image-webpack-loader": "^3.2.0",
    "jest": "18.1.0",
    "koa-webpack-dev-middleware": "^1.4.4",
    "koa-webpack-hot-middleware": "^1.0.3",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "mocha": "^2.4.5",
    "open": "0.0.5",
    "path": "^0.12.7",
    "postcss-loader": "^1.3.3",
    "react-hot-loader": "^1.3.1",
    "react-test-renderer": "15.4.2",
    "style-loader": "^0.13.1",
    "supertest": "^0.12.1",
